// my sql

USE mydatabase;

CREATE TABLE todo_items (
    id INT AUTO_INCREMENT PRIMARY KEY,
    description VARCHAR(255),
    title VARCHAR(255),
    status VARCHAR(50),
    created_at DATETIME,
    updated_at DATETIME
);

